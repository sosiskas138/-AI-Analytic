# Docker Compose
COMPOSE_PROJECT_NAME=telemarketing

# Порты (хост:контейнер)
POSTGRES_PORT=5432
BACKEND_PORT=3001
APP_PORT=8080

# База данных
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=telemarketing_analytics

# Бэкенд (для production задайте свои значения)
JWT_SECRET=local-dev-secret-change-in-production
JWT_EXPIRES_IN=7d
NODE_ENV=development
CORS_ORIGIN=http://localhost,http://localhost:8080,http://127.0.0.1:8080

# Frontend API URL (для VPS: https://ваш-домен.com/api)
VITE_API_URL=http://localhost:8080/api
